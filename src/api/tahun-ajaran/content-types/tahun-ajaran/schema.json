{
  "kind": "collectionType",
  "collectionName": "tahun_ajarans",
  "info": {
    "singularName": "tahun-ajaran",
    "pluralName": "tahun-ajarans",
    "displayName": "Tahun Ajaran"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tahunAjaran": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^\\d{4}/\\d{4}$"
    },
    "semester": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "GANJIL",
        "GENAP"
      ]
    },
    "aktif": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "riwayatKelas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::riwayat-kelas.riwayat-kelas",
      "mappedBy": "tahunAjaran"
    },
    "santris": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::santri.santri",
      "mappedBy": "tahunAjaranAktif"
    }
  }
}

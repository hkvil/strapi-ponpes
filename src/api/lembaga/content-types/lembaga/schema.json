{
  "kind": "collectionType",
  "collectionName": "lembagas",
  "info": {
    "singularName": "lembaga",
    "pluralName": "lembagas",
    "displayName": "Lembaga"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nama": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "nama"
    },
    "profilMd": {
      "type": "richtext"
    },
    "programKerjaMd": {
      "type": "richtext"
    },
    "frontImages": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "topBanner": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "botBanner": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "images": {
      "type": "component",
      "component": "profil.image-item",
      "repeatable": true
    },
    "videos": {
      "type": "component",
      "component": "profil.video-item",
      "repeatable": true
    },
    "kontak": {
      "type": "component",
      "component": "kontak.kontak",
      "repeatable": true
    },
    "news": {
      "type": "component",
      "component": "profil.berita",
      "repeatable": true
    },
    "files": {
      "type": "component",
      "component": "files.files",
      "repeatable": true
    },
    "santris": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::santri.santri",
      "mappedBy": "lembaga"
    },
    "staffs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::staff.staff",
      "mappedBy": "lembaga"
    }
  }
}
